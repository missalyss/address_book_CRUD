
{{#each onePerson}}
<div class="row">
  <hr />
  <div class="col-lg-3">
    <img class="index_img" src="{{img_url}}" alt="profile picture">
  </div>
  <div class="col-lg-9">
    <h2>{{first_name}} {{last_name}}</h2>
    <ul>
      <li>Phone: {{phone_number}}</li>
      <li>Email: {{email_address}}</li>
      <li>Address: {{line_1}}, {{line_2}}</li>

    </ul>
  </div>
</div>
{{/each}}
